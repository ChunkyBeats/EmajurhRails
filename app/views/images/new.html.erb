<h2>erhmagerd. erplerd an emaje.</h2>

<div class="forms">
  <%= simple_form_for [@image], :url => user_images_path, :html => { :multipart => true } do |form| %>

    <%= form.input :title %>
    <%= form.input :description %>
    <%= form.file_field :attachment %>
    <div id="submit">
      <%= form.submit :'Upload Image', class: "btn btn-success" %>
    </div>
<% end %>
</div>
